;;caculate distance
(begin
	(define square_sum 
		(lambda (x y)
			(+ (* x x) (* y y))))
	(define distance
		(lambda (x y)
			(sqrt 
				(square_sum 
					(- (px x) (px y)) 
					(- (py x) (py y))))))
	(print 
		(distance 
			(p 1 1)
			(p 0 0)))
)

;;recursive
(begin
	(define fact-sum
		(lambda (x)
			(if (= x 1)
				1
				(+ (self (- x 1))
					((lambda (y)
						(if (= y 1)
							1
							(* y (self (- y 1))))) x)))))
	(print
		(fact-sum 5))
)
